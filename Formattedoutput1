import java.io.*;
public class Formattedoutput1{
    public static void main(String args[]){
        String str=("usha");
        System.out.printf("The string valuse is: %s\n",str);
        int x=512;
        System.out.printf("\n The integer value is :%d\n",x);
        double f=5.254122368;
        System.out.printf("\n decimal value is %lf\n",f);
        System.out.printf("\n after formatting two Specific width the value is :%.4lf\n",f);
        System.out.printf("\n after formatted to right margin tje value is :20.4lf\n",f);
    }
}
